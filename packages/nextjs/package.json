{
  "name": "@veritas/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit --incremental",
    "dev": "next dev",
    "format": "prettier --write . '!(node_modules|.next|contracts)/**/*'",
    "lint": "next lint",
    "serve": "next start",
    "start": "next dev",
    "nillion:compile-script": "tsc scripts/create-nillion-collections.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "nillion:create-collections": "pnpm nillion:compile-script && node scripts/create-nillion-collections.js",
    "nillion:compile-test": "tsc scripts/test-nillion-collections.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "nillion:test-collections": "pnpm nillion:compile-test && node scripts/test-nillion-collections.js",
    "nillion:compile-api-test": "tsc scripts/test-api-routes.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "nillion:test-api": "pnpm nillion:compile-api-test && node scripts/test-api-routes.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "compile:set-role": "tsc scripts/set-user-role.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "set-role": "pnpm compile:set-role && node scripts/set-user-role.js",
    "compile:add-superadmin": "tsc scripts/add-superadmin.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "add-superadmin": "pnpm compile:add-superadmin && node scripts/add-superadmin.js",
    "compile:clear-db": "tsc scripts/clear-database.ts --outDir scripts --module esnext --moduleResolution bundler --target es2022 --esModuleInterop --skipLibCheck",
    "clear-db": "pnpm compile:clear-db && node scripts/clear-database.js"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
    "@nillion/blindfold": "^0.1.0",
    "@nillion/nuc": "^0.1.1",
    "@nillion/secretvaults": "latest",
    "@prisma/client": "^6.17.1",
    "@reown/appkit": "^1.8.10",
    "@reown/appkit-adapter-wagmi": "^1.8.10",
    "@reown/appkit-siwe": "^1.8.10",
    "@tanstack/react-query": "^5.90.2",
    "@veritas/types": "workspace:*",
    "@wagmi/core": "^2.22.0",
    "circomlibjs": "^0.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "siwe": "^3.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "usehooks-ts": "^3.1.1",
    "uuid": "^11.0.5",
    "viem": "^2.38.0",
    "wagmi": "^2.17.5",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "24.7.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-prettier": "~5.5.4",
    "postcss": "8.5.6",
    "prettier": "~3.6.2",
    "prisma": "^6.17.1",
    "tailwindcss": "4.1.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.15.0"
}
